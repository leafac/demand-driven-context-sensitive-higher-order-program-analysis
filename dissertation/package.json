{
  "scripts": {
    "start": "nodemon --watch src --ext '*' --exec 'npm run build'",
    "develop": "gatsby develop",
    "build": "gatsby build && node_modules/prince/prince/lib/prince/bin/prince --fileroot public --pdf-profile PDF/A-1b --no-artificial-fonts --fail-dropped-content --fail-missing-resources --fail-missing-glyphs public/index.html --output yocto-cfa.pdf",
    "clean": "gatsby clean && rm yocto-cfa.pdf",
    "test": "npm run build && prettier src"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "gatsby": "^2.20.28",
    "gatsby-plugin-mdx": "^1.1.9",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-remark-vscode": "^2.0.3",
    "gatsby-source-filesystem": "^2.2.4",
    "katex": "^0.11.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "prince": "^1.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "rehype-katex": "^3.0.0",
    "remark-math": "^2.0.1",
    "typeface-pt-mono": "0.0.72",
    "typeface-pt-serif": "0.0.72"
  }
}
